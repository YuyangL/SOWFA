FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include        "../setUp"

RASModel        $RASModel;

turbulence      on;

printCoeffs     on;

kEpsilonABLCoeffs
{
    Cmu              $Cmu;  // Turbulent eddy viscosity coefficient
    C1              $C1;  // Coefficient of the production term
    C2              $C2;
    sigmaEps        $sigmaEps;
    TName           "T";
    kappatName      "kappat";
}


