FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../setUp"


dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000765;

boundaryField
{
    lower
    {
        type            epsilonWallFunction;
        value           uniform 0.000765;
    }
    upper
    {
        type            fixedValue;
        value           uniform 0.000765;
    }
    east
    {
        type            inletOutlet;
    }
    west
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    1;  // ?
        k               "k";
        value           uniform 200;
    }
    south
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    1;  // ?
        k               "k";
        value           uniform 200;
    }
    north
    {
        type            inletOutlet;
    }
}


